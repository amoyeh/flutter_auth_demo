{
   "files.exclude": {
      ".dart_tool/": true,
      ".flutter-plugins": true,
      ".flutter-plugins-dependencies": true,
      ".metadata": true,
      ".gitignore": true,
      "pubspec.lock": true,
      ".idea": true
   },
   "dart.lineLength": 250,
   "[dart]": {
      "editor.defaultFormatter": "Dart-Code.dart-code",
      "editor.formatOnSave": true,
      "editor.tabSize": 3,
      "editor.rulers": [250]
   }
}
